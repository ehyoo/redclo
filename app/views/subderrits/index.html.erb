<p id="notice"><%= notice %></p>

<h1>Listing Subderrits</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subderrits.each do |subderrit| %>
      <tr>
        <td><%= link_to "#{subderrit.name}", subderrit %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if logged_in? %>
  <%= link_to 'New Subderrit', new_subderrit_path %>
<%end%>
